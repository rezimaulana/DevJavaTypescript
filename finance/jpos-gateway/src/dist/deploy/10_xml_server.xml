<server class="org.jpos.q2.iso.QServer" logger="Q2"
        name="xml-server-8000" realm="xml-server-8000">
    <attr name="port" type="java.lang.Integer">8000</attr>
    <channel class="link.rezimaulana.jpos.gateway.channel.Channel"
            packager="org.jpos.iso.packager.GenericPackager"
            type="server"
            logger="Q2"
            realm="xml-server-8000">
        <property name="timeout" value="180000"/>
        <property name="packager-config" value="cfg/packager.xml" />
    </channel>
    <request-listener class="link.rezimaulana.jpos.gateway.listener.LogonListener"/>
</server>

